<h1><%= t(:editing_item)%></h1>

<%= error_messages_for :item %>
<%= render :partial => 'survey/surveys/survey_options' %>


<% form_for(:item, :url => object_url(@survey), :html => { :method => :put }) do |f| %>
  <%= render :partial => "#{@item_type}_form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t(:update)%>
  </p>
<% end %>

