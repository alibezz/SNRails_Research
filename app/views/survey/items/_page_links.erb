<%= paginate_survey(@survey, params[:page] || 1) %>

<%= 
  sortable_element 'page_links', 
      :url => { :action => 'reorder_pages', :page => params[:page]},
      :failure => "$('error').innerHTML= request.responseText",
      :constraint => false,
      :dropOnEmpty => true,
      :loading => "$('mask').show(); $('loading').show();",
      :complete => visual_effect(:highlight, 'page_links') + "$('mask').hide(); $('loading').hide();", 
      :containment =>  ['page_links']
%>

