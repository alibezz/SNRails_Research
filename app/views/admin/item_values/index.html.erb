<h1><%= @research.title %></h1>

<h2><%= @research.subtitle %></h2>

<%= render :partial => 'admin/researches/research_options' %>
<% unless @research.is_active %>
  <%= link_to t(:new_item_value), new_admin_question_item_value_url(@item) %><br>
<% end %>

<%=  t(:item_values) %>

<ul>
<%- @item_values.each do |item_value| %>
  <li> 
    <%= link_to item_value.info, admin_question_item_value_url(@item, item_value) %>
  </li>
<% end %>
</ul>
