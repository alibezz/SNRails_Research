<h2><%= t(:item_values) %></h2>

<%= error_messages_for :item_value %>

<%= render :partial => "list_item_values", :locals => {:item => @item} %>

<% form_for([:admin, @item, @item_value]) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= submit_tag t(:create) %>
  </p>
<% end %>
<br/>
<%= link_to t(:item_values), admin_research_question_item_values_url(@item.research_id, @item) %>

